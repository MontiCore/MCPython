package de.monticore;

grammar SIPython extends de.monticore.SIUnitLiterals, de.monticore.Python {

  start PythonScript ;

  SIUnitConversion implements VariableInit, Expression = SIUnit "(" Expression ")" ;

  @Override
  FunctionDeclaration implements Function, Statement = "def" Name "(" FunctionDeclarationArguments ")" ":" EOL StatementBlock;
}
